{
  "name": "node-crud-api",
  "version": "1.0.0",
  "description": "simple node.js crud app",
  "main": "index.js",
  "type": "module",
  "exports": "./build/index.js",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "start:prod": "npm run build && node build/index.js",
    "start:dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enmalafeev/node-crud-api.git"
  },
  "keywords": [
    "node",
    "crud"
  ],
  "author": "Evgeny Malafeev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enmalafeev/node-crud-api/issues"
  },
  "homepage": "https://github.com/enmalafeev/node-crud-api#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
